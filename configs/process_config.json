{
    "batch": {
        "host": "jobmanager",
        "port:": "8081",
        "process_list": [
            "generate_market_info",
            "generate_vehicles",
            "generate_orders",
            "generate_images",
            "generate_customers",
            "generate_auth_types",
            "generate_improves",
            "generate_media_type",
            "generate_statuses"
        ],
        "process_command": [
            "./bin/flink",
            "run",
            "--detached",
            "--jobmanager",
            "jobmanager:8081",
            "-pyclientexec",
            "/usr/bin/python",
            "-pyexec",
            "/usr/bin/python",
            "--pyFiles",
            "/opt/flink/ops",
            "--pyModule"
        ]
    },
    "stream": {
        "host": "jobmanager",
        "port:": "8081",
        "process_list": [
            "aggregate_images",
            "enrich_full_order",
            "enrich_full_vehicle",
            "full_ad_enrich"
        ],
        "process_command": [
            "./bin/flink",
            "run",
            "--detached",
            "--jobmanager",
            "jobmanager:8081",
            "-pyclientexec",
            "/usr/bin/python",
            "-pyexec",
            "/usr/bin/python",
            "--pyFiles",
            "/opt/flink/ops",
            "--pyModule"
        ]
    }
}