                "`HorsePower` INT,",
                "`CruseControl` TINYINT,",
                "`PowerWheel` TINYINT,",
                "`FullyAutonomic` TINYINT,",
                "PRIMARY KEY (MarketInfoId) NOT ENFORCED"
            ],
            "connector_config": [
                "'url' = 'jdbc:mysql://db:3306/production',",
                "'table-name' = 'MarketInfo',",
                "'username'='root',",
                "'password'='password',",
                "'sink.parallelism' = '2',",
                "'sink.buffer-flush.interval' = '0',",
                "'sink.buffer-flush.max-rows' = '10',",
                "'sink.max-retries' = '10'"
